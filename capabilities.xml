<themeCapabilities>   
   <themeName>Min MinUI</themeName>

   <fontSize>small</fontSize>
   <fontSize>medium</fontSize>
   <fontSize>large</fontSize>

   <variant name="default">
        <label language="en_US">Text Only</label>
        <selectable>true</selectable>
   </variant>
   <variant name="boxart">
        <label language="en_US">Text with Box Art</label>
        <mediaType>cover</mediaType>
        <selectable>true</selectable>
    </variant>

   <colorScheme name="letter-prompts">
        <label language="en_US">Letters (Nintendo, Xbox)</label>
    </colorScheme>
   <colorScheme name="symbol-prompts">
        <label language="en_US">Symbols (PlayStation)</label>
    </colorScheme>
   <colorScheme name="no-prompts">
        <label language="en_US">No Prompts</label>
    </colorScheme>

   <aspectRatio>19.5:9</aspectRatio>
   <aspectRatio>16:9</aspectRatio>
   <aspectRatio>16:10</aspectRatio>
   <aspectRatio>3:2</aspectRatio>
   <aspectRatio>4:3</aspectRatio>
   <aspectRatio>5:4</aspectRatio>
   <aspectRatio>8:7</aspectRatio>
   <aspectRatio>1:1</aspectRatio>

   <transitions name="transitions">
      <label>transitions</label>
      <selectable>false</selectable>
      <systemToSystem>instant</systemToSystem>
      <systemToGamelist>instant</systemToGamelist>
      <gamelistToGamelist>instant</gamelistToGamelist>
      <gamelistToSystem>instant</gamelistToSystem>
      <startupToSystem>instant</startupToSystem>
      <startupToGamelist>instant</startupToGamelist>
   </transitions>

   <suppressTransitionProfiles>
      <entry>builtin-instant</entry>
      <entry>builtin-slide</entry>
      <entry>builtin-fade</entry>
   </suppressTransitionProfiles>
</themeCapabilities>
